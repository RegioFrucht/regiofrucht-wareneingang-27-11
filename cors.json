{
  "cors": [
    {
      "origin": ["*"],
      "method": ["GET", "HEAD", "PUT", "POST", "DELETE", "OPTIONS"],
      "maxAgeSeconds": 3600,
      "responseHeader": [
        "Content-Type",
        "Authorization",
        "Content-Length",
        "User-Agent",
        "x-goog-resumable"
      ]
    }
  ]
}